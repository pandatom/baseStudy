docker:

docker run \
  --name myNginx \
  -d -p 9001:80 \
  -v /Users/workspace/docker/nginx/service/html:/usr/share/nginx/html \
  -v /Users/workspace/docker/nginx/service/conf/nginx.conf:/etc/nginx/nginx.conf \
  -v /Users/workspace/docker/nginx/service/log/error.log:/var/log/nginx/error.log \
  nginx

docker run \
  --name myNginx01 \
  -d -p 9002:80 \
  -v /Users/workspace/docker/nginx/service01/html:/usr/share/nginx/html \
  -v /Users/workspace/docker/nginx/service01/conf/nginx.conf:/etc/nginx/nginx.conf \
  -v /Users/workspace/docker/nginx/service01/log/error.log:/var/log/nginx/error.log \
  nginx

docker run \
  --name myNginx02 \
  -d -p 9003:80 \
  -v /Users/workspace/docker/nginx/service02/html:/usr/share/nginx/html \
  -v /Users/workspace/docker/nginx/service02/conf/nginx.conf:/etc/nginx/nginx.conf \
  -v /Users/workspace/docker/nginx/service02/log/error.log:/var/log/nginx/error.log \
  nginx



mysql:
    docker run -d -p 3307:3306 --privileged=true \
    -v /Users/workspace/docker/mysql/log:/var/log/mysql \
    -v /Users/workspace/docker/mysql/data:/var/lib/mysql \
    -v /Users/workspace/docker/mysql/conf:/etc/mysql/conf.d \
    -e MYSQL_ROOT_PASSWORD=123456  \
    --platform=linux/amd64 \
    --name mysql-test mysql:5.7


redis:
   docker run  -p 6379:6379 --name myredis --privileged=true \
   -v /Users/workspace/docker/redis/conf/redis.conf:/etc/redis/redis.conf \
   -v /Users/workspace/docker/redis/data:/data \
   -d redis:6.0.8 redis-server /etc/redis/redis.conf




docker run -d -p 20:20 -p 21:21 -p 21100-21110:21100-21110 \
-v /Users/changxiong/Downloads/temp/ftp-file:/home/vsftpd \
-e FTP_USER=user \
-e FTP_PASS=userpwd \
-e PASV_ADDRESS=192.168.199.133 \
-e PASV_MIN_PORT=21100 \
-e PASV_MAX_PORT=21110 \
--name vsftpd \
--restart=always fauria/vsftpd


开起匿名访问功能
	1. vim /etc/vsftpd/vsftpd.conf
	2. 修改 anonymous_enable=YES
	3. 重启，然后 cd /var/ftp/ 查看
















